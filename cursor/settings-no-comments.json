{
  "vim.leader": " ",
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.useSystemClipboard": false,
  "vim.easymotion": true,
  "vim.smartRelativeLine": true,
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.surround": true,
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.duration": 300,
  "vim.history": 1000,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#5f87af",
  "vim.statusBarColors.insert": "#5f5f87",
  "vim.statusBarColors.visual": "#af5f87",
  "vim.statusBarColors.visualline": "#af875f",
  "vim.statusBarColors.visualblock": "#87af5f",
  "vim.statusBarColors.replace": "#af5f5f",
  "vim.searchHighlightColor": "rgba(250, 250, 50, 0.3)",
  "vim.replaceWithRegister": true,
  "vim.foldfix": true,
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.visual": "block-outline",
  "vim.cursorStylePerMode.visualline": "line-thin",
  "vim.cursorStylePerMode.visualblock": "block-outline",
  "vim.cursorStylePerMode.replace": "underline",
  "editor.lineNumbers": "relative",
  "workbench.list.automaticKeyboardNavigation": false,
  "vim.easymotionMarkerFontFamily": "monospace",
  "vim.easymotionMarkerFontSize": "16",
  "vim.easymotionMarkerForegroundColorOneChar": "#ff0000",
  "vim.easymotionMarkerForegroundColorTwoChar": "#00ff00",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-u>": true,
    "<C-f>": true,
    "<C-b>": true,
    "<C-n>": true,
    "<C-p>": true,
    "<C-a>": true,
    "<C-h>": true,
    "<C-j>": true,
    "<C-k>": true,
    "<C-l>": true
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "commands": [":write"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":quit"]
    },
    {
      "before": ["<leader>", "Q"],
      "commands": [":qa!"]
    },
    {
      "before": ["<leader>", "wq"],
      "commands": [":wq"]
    },
    {
      "before": ["<leader>", "<space>"],
      "commands": [":nohlsearch"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"+", "p"]
    },
    {
      "before": ["<leader>", "P"],
      "after": ["\"+", "P"]
    },
    {
      "before": ["<leader>", "bn"],
      "commands": [":bnext"]
    },
    {
      "before": ["<leader>", "bp"],
      "commands": [":bprevious"]
    },
    {
      "before": ["<leader>", "bd"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "D"],
      "commands": ["editor.action.goToDeclaration"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["g", "y"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<leader>", "rn"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "ca"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "ff"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "fg"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "fb"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["<C-n>"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    },
    {
      "before": ["<C-p>"],
      "commands": ["editor.action.addSelectionToPreviousFindMatch"]
    },
    {
      "before": ["g", "b"],
      "commands": ["editor.action.selectHighlights"]
    },
    {
      "before": ["<C-d>"],
      "commands": ["editorScroll", { "to": "down", "by": "halfPage" }]
    },
    {
      "before": ["<C-u>"],
      "commands": ["editorScroll", { "to": "up", "by": "halfPage" }]
    },
    {
      "before": ["<C-a>"],
      "commands": ["editor.action.selectAll"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<A-j>"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["<A-k>"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["<leader>", "y"],
      "after": ["\"+", "y"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["\"+", "d"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"+", "p"]
    },
    {
      "before": ["<leader>", "P"],
      "after": ["\"+", "P"]
    },
    {
      "before": ["<C-n>"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    },
    {
      "before": ["<C-p>"],
      "commands": ["editor.action.addSelectionToPreviousFindMatch"]
    },
    {
      "before": ["<C-d>"],
      "commands": ["editorScroll", { "to": "down", "by": "halfPage" }]
    },
    {
      "before": ["<C-u>"],
      "commands": ["editorScroll", { "to": "up", "by": "halfPage" }]
    }
  ]
} 